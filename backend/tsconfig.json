{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "lib": ["ES6"],
      "rootDir": "./src",
      "outDir": "./dist",
      "esModuleInterop": true,
      "strict": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    }
  }
  